{
  "name": "Front-End-Boilerplate",
  "version": "2.0.2",
  "description": "Yet Another Boilerplate you'll have to modify the heck out of",
  "author": "Chris W. Burke",
  "engines": {
    "node": "6.7.0",
    "npm": "3.10.9"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build": "node ./bin/tasks/build",
    "build:production": "node ./bin/tasks/build production",
    "test": "npm run build",
    "clean": "node ./bin/tasks/clean",
    "assemble": "node ./bin/tasks/assemble",
    "sass": "node ./bin/tasks/sass && node ./bin/tasks/postcss",
    "sass:doc": "sassdoc src/sass -d docs",
    "serve": "node ./bin/tasks/serve",
    "images": "node ./bin/tasks/images",
    "sprites": "node ./bin/tasks/sprites",
    "iconfont": "node ./bin/tasks/iconfont",
    "copy": "node ./bin/tasks/copy",
    "webpack": "node ./bin/tasks/webpack",
    "watch": "node ./bin/tasks/watch",
    "setup": "./bin/setup.sh",
    "start": "node ./bin/tasks/serve",
    "postinstall": "./bin/postinstall.sh",
    "dev": "npm run watch & node ./bin/tasks/browser-sync",
    "deploy": "./bin/deploy.sh dev || true",
    "deploy:qa": "./bin/deploy.sh qa || true",
    "deploy:staging": "./bin/deploy.sh staging || true"
  },
  "dependencies": {
    "assemble": "^0.18.1",
    "autoprefixer": "^6.7.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "browser-sync": "^2.18.6",
    "chalk": "^1.1.3",
    "copy": "^0.3.0",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.10.0",
    "data-attributes": "^1.0.0",
    "dom-helpers": "^3.2.1",
    "domready": "^1.0.8",
    "fs-extra": "^2.0.0",
    "gaze": "^1.1.2",
    "glob": "^7.1.1",
    "glob-fs": "^0.1.6",
    "gulp-extname": "^0.2.2",
    "handlebars-helpers": "^0.8.0",
    "imagemin": "^5.2.2",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-optipng": "^5.2.1",
    "imagemin-pngquant": "^5.0.0",
    "imagemin-svgo": "^5.2.0",
    "jquery": "^3.1.1",
    "log4js": "^1.1.0",
    "lwip": "0.0.9",
    "map-stream": "0.0.6",
    "node-sass": "^4.3.0",
    "node-sass-import": "^1.1.1",
    "npm-watch": "^0.1.7",
    "onchange": "^3.2.1",
    "plur": "^2.1.2",
    "postcss": "^5.2.11",
    "pretty-bytes": "^4.0.2",
    "pretty-log": "^0.1.0",
    "rimraf": "^2.5.4",
    "sass-lint": "^1.10.2",
    "sassdoc": "^2.1.20",
    "shelljs": "^0.7.6",
    "sprity": "^1.0.8",
    "sprity-sass": "^1.0.4",
    "superstatic": "^4.0.3",
    "through2-concurrent": "^1.1.1",
    "vinyl-fs": "^2.4.4",
    "waypoints": "^4.0.1",
    "webfonts-generator": "^0.3.5",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "source-component": "^1.2.1"
  }
}
